From 12d9d787a22a7b75706d4d8bfa1b729d917a04be Mon Sep 17 00:00:00 2001
From: Romain Pokrzywka <romain.pokrzywka@kdab.com>
Date: Thu, 13 Sep 2012 17:17:23 -0700
Subject: [PATCH] fix GCC bug with hidden inlined symbols

---
 src/qml/qml.pro |    1 +
 1 file changed, 1 insertion(+)

diff --git a/../qt5-src/qtdeclarative/src/qml/qml.pro b/../qt5-src/qtdeclarative/src/qml/qml.pro
index 620db59..102030c 100644
--- a/../qt5-src/qtdeclarative/src/qml/qml.pro
+++ b/../qt5-src/qtdeclarative/src/qml/qml.pro
@@ -9,6 +9,7 @@ DEFINES   += QT_NO_URL_CAST_FROM_STRING QT_NO_INTEGER_EVENT_COORDINATES
 win32-msvc*|win32-icc:QMAKE_LFLAGS += /BASE:0x66000000
 win32-msvc*:DEFINES *= _CRT_SECURE_NO_WARNINGS
 solaris-cc*:QMAKE_CXXFLAGS_RELEASE -= -O2
+linux-archos-gen8-g++:QMAKE_CXXFLAGS_RELEASE += -fno-inline
 
 exists("qqml_enable_gcov") {
     QMAKE_CXXFLAGS = -fprofile-arcs -ftest-coverage -fno-elide-constructors
-- 
